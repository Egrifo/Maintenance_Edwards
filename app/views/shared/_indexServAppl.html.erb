

  <%#= link_to 'New appliance', new_appliance_path %>
            <div class="col-lg-12 mb-6">
              <!-- Project Card Example -->
              <div class="card shadow mb-6">
                <div class="card-header py-3">
                  <h6 class="m-0 font-weight-bold text-primary"><%= @title_index %></h6>
                </div>
              <table class="table table-bordered table-hover" id="dataTable" width="100%" cellspacing="0">
                <thead>
                  <tr>
                    <th>Name</th>
                    <th>Brand</th>
                    <th>Model</th>
                    <th>Tested</th>
                    <th>Market Value</th>
                    <th colspan="2"><%= link_to fa_icon("plus", text: "Create"), new_appliance_path %></th>
                
                  </tr>
                  </thead>
                  <tbody>
                    <% @appliances.each do |appliance| %>
                      <tr>
                        <td><%= appliance.name %></td>
                        <td><%= appliance.brand %></td>
                        <td><%= appliance.model %></td>
                        <td><%= appliance.tested %></td>
                        <td><%= appliance.purchasing_cost %>â‚¬</td>
                        <td><%= link_to fa_icon('trash', text: "Delete"), appliance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                      </tr>
                    <% end %>
                  </tbody>
                </table>

              <%#= @services.user%>
              </div>
            </div>